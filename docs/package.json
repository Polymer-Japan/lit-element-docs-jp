{
  "private": true,
  "scripts": {
    "build": "polymer build && jekyll build",
    "build-all": "(cd .. && npm run gen-docs) && npm run build",
    "gen-api-docs-init-patch": "patch -p0 < _api/ja/typedoc-plugin-localization-render.diff",
    "gen-api-docs-en": "(cd _api && sh copy_api.sh)  && cp ../tsconfig.json _api/en && typedoc --params --generate-json ./_api/en --exclude '{**/*test*,**/node_modules/**,**/test/**}' _api/tmp/**/*.ts",
    "gen-api-docs-ja": "typedoc --readme _api/api-readme.md  --mode modules --theme _api/theme --tsconfig _api/en/tsconfig.json --ignoreCompilerErrors --excludeNotExported --excludePrivate --out ./api --gaID UA-131004707-1 --generate-from-json ./_api/ja --localize ja --templateStrings ./_api/ja/shell-strings.json --params  --exclude '{**/*test*,**/node_modules/**,**/test/**}' _api/tmp/**/*.ts",
    "serve": "dev_appserver.py _site/app.yaml",
    "deploy": "npm run build && gcloud app deploy _site/app.yaml --project polymer-lit-element --no-promote"
  },
  "dependencies": {
    "@polymer/lit-element": "^0.6.2",
    "@stackblitz/sdk": "^1.2.0",
    "@webcomponents/webcomponentsjs": "^2.1.3"
  },
  "devDependencies": {
    "typedoc": "^0.13.0",
    "typedoc-plugin-external-module-name": "^1.1.3",
    "typedoc-plugin-localization": "^1.4.1"
  }
}
